    "def reverse_str(str1):\n",
    "    reverse_string = \" \"\n",
    "    index = len(str1)\n",
    "    while index > 0:\n",
    "        reverse_string = reverse_string+str1[index-1]\n",
    "        index = index - 1\n",
    "    return reverse_string\n",
    "output = reverse_str(\"1234abcd\")\n",
    "print(output)"
   ]
